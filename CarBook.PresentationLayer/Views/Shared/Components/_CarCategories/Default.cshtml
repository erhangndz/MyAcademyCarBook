@using CarBook.BusinessLayer.Dtos;
@model List<CategoryDto>


<div class="tab-pane fade" id="pills-categories" role="tabpanel" aria-labelledby="pills-review-tab">
	<div class="row">
		<div class="col-md-7">
			<h3 class="head">Araç Kategorileri</h3>
			
			
				@foreach (var item in Model)
			{
				
			
			
				<div class="review d-flex">
					
					<div class="desc">
						<h5>
							<p class="text-left">@item.CategoryName  (<strong>@item.Count</strong>)</p>
							
						</h5>
						
						
					</div>
				</div>
			}
			
		</div>
		
	</div>
</div>